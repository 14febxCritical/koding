{
  "name"          : "WordpressInstaller",
  "version"       : "0.2",
  "author"        : "Sinan Yasar",
  "repo"          : "git://github.com/sinanyasar/WordpressInstaller.git",
  "description"   : "This is a Wordpress Installer for Koding",
  "path"          : "~/Applications/WordPress.kdapp/",
  "source"        : {
    "blocks"      : {
      "app"       : {
        "files"   : [
          "./functions.coffee",
          "./common.coffee",
          "./dashboardpane.coffee",
          "./installpane.coffee",
          "./main.coffee"
        ]
      }
    },
    "stylesheets" : [
      "./resources/style.css"
    ]
  },
  "options"       : {
    "type"        : "tab"
  },
  "icns"          : {
    "256"         : "./resources/wp2.png"
  }
}
