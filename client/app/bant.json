{
  "name": "app",
  "main": "./lib/index.coffee",
  "routes": {
    "": "home",
    "/": "home",
    "/R/:username": "referrer",
    "/Teams/:token?": "teams",
    "/Login": "login",
    "/Register": "register",
    "/Logout": "logout",
    "/logout": "logout",
    "/:slug/:name": "members",
    "/member/:username": "member",
    "/Unsubscribe/:email/:opt?": "unsubscribe",
    "/:name": "name",
    "/Reset/:token": "reset",
    "/Shortcuts": "shortcuts",
    "/Collaboration/:nickname/:channelId": "request-collaboration"
  },
  "sprites": [
    "./lib/sprites"
  ],
  "styles": [
    "./lib/styl/*.styl",
    "./lib/styl/reactivity/*.styl",
    "./lib/styl/terminal/*.styl",
    "./lib/components/**/styl/*.styl"
  ]
}
