CFG=kodingme
RGN=kodingme
HST=localhost
PBKEY=./certs/test_kontrol_rsa_public.pem
PVKEY=./certs/test_kontrol_rsa_private.pem

go run ./go/src/github.com/koding/kite/kontrol/kontrol/main.go -init -public-key ./certs/test_kontrol_rsa_public.pem -private-key ./certs/test_kontrol_rsa_private.pem -username koding  -kontrol-url "http://localhost:4000" 

		 ./go/bin/kontrol        				 -c $CFG -r $RGN
		 ./go/bin/kloud          				 -c $CFG -r $RGN -public-key $PBKEY -private-key $PVKEY -kontrol-url "http://$HST:4000/kite"
		 ./go/bin/rerouting      				 -c $CFG
		 ./go/bin/cron           				 -c $CFG
		 ./go/bin/broker         				 -c $CFG
		 ./go/bin/reverseproxy   				 -region $RGN -host $HST -env production 
node ./server/index.js               -c $CFG -p 80   --disable-newrelic
node ./server/lib/source-server.js   -c $CFG -p 3526
node ./workers/auth/index.js         -c $CFG
node ./workers/social/index.js       -c $CFG -p 3030 --disable-newrelic --kite-port=13020
node ./workers/guestcleaner/index.js -c $CFG
node ./workers/emailsender/index.js  -c $CFG


