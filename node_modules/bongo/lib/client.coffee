Model = require './model'
ObjectId = require './objectid'
module.exports = class Client
  constructor:(@connection)->
  
  is:(strOrId)->
    {delegate} = @connection
    if 'string' is typeof strOrId
      delegate.getId().toString() is strOrId
    else if strOrId instanceof ObjectId or strOrId = strOrId?.getId?()
      delegate.getId().equals strOrId