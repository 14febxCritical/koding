SaxParser = require './saxparser'

class KDMLParser extends SaxParser
	constructor:->
		super

		@context = null

		@on 'elementstart', console.log
		@on 'elementend', console.log


if module?.exports?
  module.exports.KDMLParser = KDMLParser
else if define?.amd?
  define -> KDMLParser
else if window?
  window['KDMLParser'] = KDMLParser   # export as a string to play nice with Google Closure Compiler
