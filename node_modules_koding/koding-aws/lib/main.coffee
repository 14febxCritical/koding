awsKey = ''
awsSecret = ''

buildTemplate = (config) ->
  console.log "Building AWS files..."
  console.log ""
  console.log "Running instane on AWS EC2..."
  console.log ""

printTemplate = (config) ->
  console.log "AMI   : ", config.ami
  console.log "Type  : ", config.type
  console.log ""

AWS =
  init: (config) ->
    awsKey = config.key
    awsSecret = config.secret

  print: printTemplate
  build: buildTemplate

module.exports = AWS