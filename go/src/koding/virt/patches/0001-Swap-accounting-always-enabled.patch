index 370d8a5..b0b1f45 100644
--- a/debian.master/config/config.common.ubuntu
+++ b/debian.master/config/config.common.ubuntu
@@ -3375,7 +3375,7 @@ CONFIG_MEGARAID_SAS=m
 CONFIG_MEMCG=y
 CONFIG_MEMCG_KMEM=y
 CONFIG_MEMCG_SWAP=y
