{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Koding",
  "description": "This is not koding",
  "generators": [
    {
      "sql-definition": {
        "roleName": "koding_role",
        "grants": [
          "SELECT",
          "INSERT",
          "DELETE",
          "UPDATE"
        ],
        "databaseName": "koding_db",
        "schemaName": "koding_schema"
      }
    }
  ],
  "definitions": {
    "Config": {
      "type": "config",
      "title": "Config",
      "description": "Config represents the required options for this module to work",
      "definitions": {},
      "properties": {
        "Postgres": {
          "type": "object",
          "title": "Postgres",
          "description": "Postgres holds the all credentials for postgres db connection.",
          "properties": {
            "Port": {
              "description": "The port number for postgres config",
              "type": "number",
              "format": "int64",
              "minimum": 1024
            },
            "Host": {
              "description": "Host holds the hostname for the postgres",
              "type": "string",
              "minLength": 4,
              "maxLength": 24
            },
            "Username": {
              "description": "Username holds the username for the postgres",
              "type": "string",
              "minLength": 4,
              "maxLength": 24
            },
            "Password": {
              "description": "Password holds the password for the postgres",
              "type": "string",
              "minLength": 4,
              "maxLength": 24
            },
            "DBName": {
              "description": "DBName holds the database name for the postgres",
              "type": "string",
              "minLength": 4,
              "maxLength": 24
            }
          },
          "definitions": {}
        }
      }
    },
    "Machine": {
      "type": "object",
      "title": "Machine",
      "description": "Machine represents a registered Account's Machine Info",
      "generators": [
        {
          "sql-definition": {
            "grants": [
              "SELECT",
              "INSERT",
              "UPDATE"
            ],
            "primaryKey": [
              "Id"
            ]
          }
        }
      ],
      "properties": {
        "Id": {
          "description": "The unique identifier for a Machine",
          "type": "number",
          "format": "int64",
          "minimum": 1,
          "propertyOrder": 10
        },
        "Username": {
          "description": "Username is the user name of the machine",
          "type": "string",
          "minLength": 4,
          "maxLength": 20,
          "propertyOrder": 20
        },
        "Owner": {
          "description": "Owner return if the machine's username is owner or not.",
          "type": "boolean",
          "minLength": 0,
          "maxLength": 30,
          "propertyOrder": 30
        },
        "State": {
          "description": "State is the current status of the machine.",
          "type": "string",
          "minLength": 0,
          "maxLength": 30,
          "propertyOrder": 40
        }
      },
      "required": [
        "Username"
      ],
      "functions": {
        "Start": {
          "type": "object",
          "description": "Start returns the machine if it is started or not.",
          "title": "Start",
          "properties": {
            "incoming": {
              "type": "object",
              "title": "Start",
              "properties": {
                "Id": {
                  "description": "The unique identifier for a Machine",
                  "type": "string",
                  "minimum": 1
                }
              }
            },
            "outgoing": {
              "$ref": "#/definitions/Machine"
            }
          }
        },
        "Stop": {
          "type": "object",
          "description": "Stop returns the machine if it is stopped or not.",
          "title": "Stop",
          "properties": {
            "incoming": {
              "type": "object",
              "title": "Stop",
              "properties": {
                "Id": {
                  "description": "The unique identifier for a Machine",
                  "type": "string",
                  "minimum": 1
                }
              }
            },
            "outgoing": {
              "$ref": "#/definitions/Machine"
            }
          }
        },
        "Status": {
          "type": "object",
          "description": "Status returns the machine's current status.",
          "title": "Status",
          "properties": {
            "incoming": {
              "type": "object",
              "title": "Status",
              "properties": {
                "Id": {
                  "description": "The unique identifier for a Machine",
                  "type": "string",
                  "minimum": 1
                }
              }
            },
            "outgoing": {
              "$ref": "#/definitions/Machine"
            }
          }
        },
        "UserMachines": {
          "type": "object",
          "description": "UserMachines lists the all machines of the user.",
          "title": "UserMachines",
          "properties": {
            "incoming": {
              "type": "object",
              "title": "UserMachines",
              "properties": {
                "Username": {
                  "description": "Username of the machine",
                  "type": "string",
                  "minLength": 1,
                  "propertyOrder": 50
                }
              }
            },
            "outgoing": {
              "type": "array",
              "items": [
                {
                  "$ref": "#/definitions/Machine"
                }
              ]
            }
          }
        }
      }
    }
  }
}
