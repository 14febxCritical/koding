{
  "name"          : "JoomlaInstaller",
  "version"       : "0.1",
  "author"        : "Sinan Yasar",
  "repo"          : "git://github.com/sinanyasar/JoomlaInstaller.git",
  "description"   : "This is a Joomla Installer for Koding",
  "path"          : "~/Applications/Joomla.kdapp/",
  "source"        : {
    "blocks"      : {
