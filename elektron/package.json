{
  "name": "Koding",
  "version": "1.0.0",
  "description": "Koding Desktop App",
  "main": "./lib/main.js",
  "scripts": {
    "prestart": "coffee --compile --output lib/ src/",
    "start": "electron ./lib/main.js",
    "preapp-mac": "coffee --compile --output lib/ src/",
    "app-mac": "electron-packager . Koding --out ./dist  --platform=darwin --arch=x64 --version=0.36.6 --overwrite --icon=./assets/icons/square-logo-orange.icns",
    "preapp-win": "coffee --compile --output lib/ src/",
    "app-win": "electron-packager . Koding --out ./dist --platform=win32 --arch=ia32 --version=0.36.6 --overwrite --icon=./assets/icons/square-logo-orange.ico",
    "preapp-linux": "coffee --compile --output lib/ src/",
    "app-linux": "electron-packager . Koding --out ./dist --platform=linux --arch=x64 --version=0.36.6 --overwrite --icon=./assets/icons/square-logo-orange-linux"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koding/koding.git"
  },
  "keywords": [
    "Koding",
    "desktop",
    "app"
  ],
  "author": "Sinan Yasar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/koding/koding/issues"
  },
  "homepage": "https://github.com/koding/koding#readme",
  "devDependencies": {
    "coffee-script": "^1.10.0",
    "electron-packager": "^5.2.1",
    "electron-prebuilt": "^0.36.5"
  }
}
